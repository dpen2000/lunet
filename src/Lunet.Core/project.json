{
  "title": "Lunet.Core",
  "version": "0.1.0",
  "authors": [ "Alexandre Mutel" ],
  "copyright": "Alexandre Mutel",
  "description": "Core library for Lunet, a fast & powerful Static Website Engine",
  "language": "en-US",
  "packOptions": {
    "owners": [ "Alexandre Mutel" ],
    "projectUrl": "https://github.com/lunet-io/lunet",
    "licenseUrl": "https://github.com/lunet-io/lunet/blob/master/license.txt",
    "requireLicenseAcceptance": false,
    "tags": [ "website", "jekyll", "hugo", "static" ],
    "repository": {
      "type": "git",
      "url": "git://github.com/lunet-io/lunet"
    }
  },
  "frameworks": {
    "netstandard1.5": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.5.0-rc2-24027"
      }
    }
  },
  "buildOptions": {
    "copyToOutput": {
      "include": [ "data" ]
    }
  },
  //"publishOptions": {
  //  "include": [ "files" ]
  //},
  "dependencies": {
    "Markdig": "0.2.0",
    "Scriban": "0.1.0",
    "Microsoft.AspNetCore.Hosting": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.WebSockets.Server": "0.1.0-rc2-final",
    "Microsoft.Extensions.Logging": "1.0.0-rc2-final",
    "Microsoft.Extensions.Logging.Console": "1.0.0-rc2-final",
    "Newtonsoft.Json": "8.0.4-beta1",
    "NuGet.Versioning": "3.5.0-rc-1298",
    "NUglify": "1.4.0",
    "SharpScss": "1.1.0",
    "System.IO.Compression": "4.1.0-rc2-24027",
    "System.Net.Http": "4.0.1-rc2-24027",
    "System.Threading.Tasks.Dataflow": "4.6.0-rc2-24027"
  }
}